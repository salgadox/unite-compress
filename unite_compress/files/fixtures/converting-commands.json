[
    {
        "model": "files.convertingcommand",
        "pk": 1,
        "fields": {
            "title": "video standard",
            "mime_regex": "video/*",
            "is_enabled": true,
            "command": "ffmpeg -y -hide_banner -nostats -i %(input_file)s -vcodec libx264 -crf 25 -threads 0 -strict -2 %(output_file)s"
        }
    },
    {
        "model": "files.convertingcommand",
        "pk": 2,
        "fields": {
            "title": "pdf standard",
            "mime_regex": "application/pdf",
            "is_enabled": true,
            "command": "gs -sDEVICE=pdfwrite -dCompatibilityLevel=1.4 -dPDFSETTINGS=/screen -dNOPAUSE -dQUIET -dBATCH -sOutputFile=%(output_file)s %(input_file)s"
        }
    },
    {
        "model": "files.convertingcommand",
        "pk": 3,
        "fields": {
            "title": "image standard",
            "mime_regex": "image/*",
            "is_enabled": true,
            "command": "convert %(input_file)s -quality 60  %(output_file)s"
        }
    },
    {
        "model": "files.convertingcommand",
        "pk": 4,
        "fields": {
            "title": "pptx, ppt standard",
            "mime_regex": "(application/vnd\\.ms-powerpoint|application/vnd\\.openxmlformats-officedocument\\.presentationml\\.presentation)",
            "is_enabled": true,
            "command": "unoconv -f pdf %(input_file)s"
        }
    }
]
